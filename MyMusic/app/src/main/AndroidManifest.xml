<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.android.mymusic">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".ArtistsActivity"
            android:label="Artists"/>
        <activity android:name=".AlbumsActivity"
            android:label="Albums"/>
        <activity android:name=".PlaylistsActivity"
            android:label="Playlist"/>
        <activity android:name=".PaymentActivity"
            android:label="Payment"/>
        <activity android:name=".AttentionSongActivity"
            android:label="Now Playing"/>
        <activity android:name=".CarNachdiSongActivity"
            android:label="Now Playing"/>
        <activity android:name=".NajaSongActivity"
            android:label="Now Playing"/>
        <activity android:name=".HoorSongActivity"
            android:label="Now Playing"/>
    </application>

</manifest>